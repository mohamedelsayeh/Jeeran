    pod 'Bolts'
    pod 'FBSDKCoreKit'
    pod 'FBSDKShareKit'
    pod 'FBSDKLoginKit'
    
    # Floating Button Pods
    pod 'LiquidFloatingActionButton'
    
    # Image Slider Pods
    pod 'Auk', '~> 2.1'
    
end
